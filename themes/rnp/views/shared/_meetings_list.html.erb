<% [user, room, meetings_and_recordings, all_meetings_loaded] %>

<% meetings_and_recordings.each do |meet_rec| %>
  <%= render "shared/meeting_row", user: user, room: room, meeting: meet_rec[:meeting], recording: meet_rec[:recording].present? ? meet_rec[:recording] : nil %>
<% end %>

<% if all_meetings_loaded %>
  <%# This is just a metadata, it will not be rendered %>
  <tr data-all-loaded></tr>
<% end%>
